<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.example.library.BandCardEditText
        android:id="@+id/et"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <kaizone.songmaya.woo.widget.DelEditText
        android:id="@+id/del_et"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/submit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="submit"/>

    <Button
        android:id="@+id/idcard1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="身份证正面扫描"/>
    <Button
        android:id="@+id/idcard2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="身份证反面扫描"/>
    <Button
        android:id="@+id/liveDetect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="人脸识别"/>

</LinearLayout>